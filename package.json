{
  "name": "medical-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "json-server --watch db.json --port 4000",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview --port 5173 --strictPort",
    "test": "vitest --run",
